<%- include('partials/header', { title: "Admin Panel" }) %>
<h1>Admin Dashboard</h1>
<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Role</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user => { %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.role %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
<a href="/landing">Go back</a>
<%- include('partials/footer') %>
